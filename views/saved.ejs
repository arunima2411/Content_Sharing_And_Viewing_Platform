<% include ./partials/header.ejs %>

<div class="w-full min-h-screen bg-zinc-800 pt-1">

    <div class="cards flex flex-wrap gap-10 px-7 mt-10">
        <% user.saved.forEach(function(elem){ %>
            <a href="/feed/<%=elem._id%>"><div class="card w-32 text-white">
                <div class="w-full h-40 rounded-lg bg-zinc-200 rounded-lg overflow-hidden">
                    <img class="w-full h-full object-cover" src="/images/uploads/<%=elem.image%>" alt="">
                </div>
                
                <h5 class="text-sm mt-2"><%=elem.title%></h5>
                </div></a>
        <%}) %>
        
        </div>
        
    </div>


<% include ./partials/footer.ejs %>


